package edu.fju.exam;

public class Manager extends Employee {
    int i=40000;
    String e="Eddie";
    int n=47000;
    String a="Andy";
	public Manager(String j, int a) {
		super(j, a);
		
	}
	@Override
	public void print(){
		System.out.println("Eddie"+" "+(i+5000));
		System.out.println("Andy"+" "+(n+5000));
	}

}
