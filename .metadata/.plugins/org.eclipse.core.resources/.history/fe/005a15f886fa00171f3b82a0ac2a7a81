package edu.fju.exam;

public class Bubble {

	public static void main(String[] args) {
      int a[]={45,81,9,72,4,6};
      for(int i=0;i<a.length-1;i++){
    	  for(int n=i+1;n<a.length;n++){
    	  if(a[i]<a[n]){
    	  int tem=a[i];
    	  a[i]=a[n];
    	  a[n]=tem;
    	  }
    	  }
      }
      
      
      for(int y:a){
    	  System.out.print(y+" ");
      }
		
	}

}
