package edu.fju.exam;

public class Employee {
      int money;
      int a=25000;
      int b=28000;
      int c=31000;
      public Employee(int money){
    	  this.money=money;
      }
	
	public void print(){
		System.out.println("Jack"+a);
		System.out.println("Mary"+b);
		System.out.println("Danny"+c);
	}
}
