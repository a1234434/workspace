package edu.fju.exam;

public class Employee {
      String j="Jack";
      int a=25000;
      String m="Mary";
      int b=28000;
      String d="Danny";
      int c=31000;

	public Employee(String j,int a,String m,String d,int b,int c) {
		this.j=j;
		this.a=a;
		this.b=b;
		this.c=c;
		this.d=d;
        this.m=m;
	}

	public void print(){
		System.out.println("Jack"+" "+a);
		System.out.println("Mary"+" "+b);
		System.out.println("Danny"+" "+c);
	}
}
